{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sa2": {
      "root": "projects/sa2/",
      "sourceRoot": "projects/sa2/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sa2",
            "index": "projects/sa2/src/index.html",
            "main": "projects/sa2/src/main.ts",
            "polyfills": "projects/sa2/src/polyfills.ts",
            "tsConfig": "projects/sa2/tsconfig.app.json",
            "assets": [
              "projects/sa2/src/favicon.ico",
              "projects/sa2/src/assets"
            ],
            "styles": [
              "projects/sa2/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/sa2/src/environments/environment.ts",
                  "with": "projects/sa2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sa2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sa2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sa2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sa2/src/test.ts",
            "polyfills": "projects/sa2/src/polyfills.ts",
            "tsConfig": "projects/sa2/tsconfig.spec.json",
            "karmaConfig": "projects/sa2/karma.conf.js",
            "styles": [
              "projects/sa2/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/sa2/src/favicon.ico",
              "projects/sa2/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sa2/tsconfig.app.json",
              "projects/sa2/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "sa2-e2e": {
      "root": "projects/sa2-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/sa2-e2e/protractor.conf.js",
            "devServerTarget": "sa2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sa2:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/sa2-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "sa2-lib": {
      "root": "projects/sa2-lib",
      "sourceRoot": "projects/sa2-lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/sa2-lib/tsconfig.lib.json",
            "project": "projects/sa2-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sa2-lib/src/test.ts",
            "tsConfig": "projects/sa2-lib/tsconfig.spec.json",
            "karmaConfig": "projects/sa2-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sa2-lib/tsconfig.lib.json",
              "projects/sa2-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "atomic-component": {
      "root": "projects/atomic-component",
      "sourceRoot": "projects/atomic-component/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/atomic-component/tsconfig.lib.json",
            "project": "projects/atomic-component/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/atomic-component/src/test.ts",
            "tsConfig": "projects/atomic-component/tsconfig.spec.json",
            "karmaConfig": "projects/atomic-component/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/atomic-component/tsconfig.lib.json",
              "projects/atomic-component/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "frontal": {
      "root": "projects/frontal",
      "sourceRoot": "projects/frontal/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/frontal/tsconfig.lib.json",
            "project": "projects/frontal/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/frontal/src/test.ts",
            "tsConfig": "projects/frontal/tsconfig.spec.json",
            "karmaConfig": "projects/frontal/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/frontal/tsconfig.lib.json",
              "projects/frontal/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "rapi-chat": {
      "root": "projects/rapi-chat",
      "sourceRoot": "projects/rapi-chat/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/rapi-chat/tsconfig.lib.json",
            "project": "projects/rapi-chat/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rapi-chat/src/test.ts",
            "tsConfig": "projects/rapi-chat/tsconfig.spec.json",
            "karmaConfig": "projects/rapi-chat/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/rapi-chat/tsconfig.lib.json",
              "projects/rapi-chat/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "sa2"
}